{
  "name": "avl-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate for AVL web projects.",
  "devDependencies": {
    "browser-sync": "^2.24.6",
    "electron": "^2.0.6",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    }
  },
  "main": "main.js",
  "scripts": {
    "serve": "node_modules/.bin/browser-sync src -w",
    "electron": "node_modules/.bin/electron electron"
  }
}
